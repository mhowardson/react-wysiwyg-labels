{
  "name": "react-label-designer",
  "version": "1.0.0",
  "description": "A modular, open-source WYSIWYG label designer for React with ZPL/EPL code generation",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "label-designer",
    "wysiwyg",
    "zpl",
    "epl",
    "barcode",
    "printer",
    "zebra",
    "eltron",
    "label-printing",
    "drag-and-drop",
    "ui-framework",
    "modular",
    "typescript",
    "vite"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhowardson/react-wysiwyg-labels.git"
  },
  "bugs": {
    "url": "https://github.com/mhowardson/react-wysiwyg-labels/issues"
  },
  "homepage": "https://github.com/mhowardson/react-wysiwyg-labels#readme",
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "dependencies": {
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/addon-essentials": "^8.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "optionalDependencies": {
    "@mui/material": "^5.15.14",
    "@mui/icons-material": "^5.15.14",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "bootstrap": "^5.3.3",
    "react-bootstrap": "^2.10.2",
    "antd": "^5.16.1",
    "tailwindcss": "^3.4.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}